{
  "name": "webdriverio-testing",
  "version": "0.0.1",
  "description": "Browser Testing",
  "scripts": {
    "test": "wdio wdio.conf.js",
    "test:spec": "wdio wdio.conf.js --spec",
    "clean": "rm -rf allure-results",
