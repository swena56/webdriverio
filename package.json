    "chai-as-promised": "^7.1.1",
    "chai-webdriverio": "^0.4.3",
    "jquery": "3.2.1",
    "mocha-tags": "^1.0.1",
    "selenium-standalone": "^6.15.2",
    "wdio-allure-reporter": "^0.7.0",
    "wdio-junit-reporter": "^0.4.4",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-sauce-service": "^0.4.8",
    "wdio-spec-reporter": "^0.1.4",
